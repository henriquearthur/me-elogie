{
    "version": 2,
    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "/static/$1"
        },
        {
            "src": "/assets/(.*)",
            "dest": "/assets/$1"
        },
        {
            "src": "/favicon.ico",
            "dest": "/favicon.ico"
        },
        {
            "src": "/manifest.json",
            "dest": "/manifest.json"
        },
        {
            "src": "/pdf.worker.min.mjs",
            "dest": "/public/pdf.worker.min.mjs"
        },
        {
            "src": "/(.*)",
            "dest": "/index.html"
        }
    ]
}